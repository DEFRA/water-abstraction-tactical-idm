    "codecov": "^3.8.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.9.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^4.1.0",
    "sinon": "^12.0.1",
    "snyk": "^1.852.0"
  },
  "dependencies": {
    "@envage/hapi-pg-rest-api": "^7.0.0",
