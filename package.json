    "dotenv": "^8.6.0",
    "good-winston": "^4.0.0",
    "hapi-auth-jwt2": "^8.8.1",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "uuid": "^8.3.2"
  }
